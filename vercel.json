{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/waitlist",
        "dest": "/routes/waitlistRoutes.js"
      },
      {
        "src": "/api/resend",
        "dest": "/routes/resendRoutes.js"
      },
      {
        "src": "/health",
        "dest": "/index.js"
      },
      {
        "src": "/emailTemplate_it.html",
        "dest": "./file/emailTemplate_it.html"
      },
      {
        "src": "/emailTemplate_en.html",
        "dest": "./file/emailTemplate_en.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  