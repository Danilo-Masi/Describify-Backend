{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/waitlist",
            "dest": "/routes/waitlistRoutes.js"
        },
        {
            "src": "/api/resend",
            "dest": "/routes/resendRoutes.js"
        },
        {
            "src": "/health",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}
